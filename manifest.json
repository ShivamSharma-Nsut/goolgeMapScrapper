{
  "manifest_version": 3,
  "name": "Mr. G-Map Scrapper",
  "version": "1.3.0",
  "description": "Extract business data from Google Maps search results",
  "host_permissions": [
    "https://*.google.com/*",
    "https://*.google.co.in/*",
    "https://*.google.co.uk/*",
    "https://*.google.ca/*",
    "https://*.google.com.au/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/maps*",
        "https://*.google.co.in/maps*",
        "https://*.google.co.uk/maps*",
        "https://*.google.ca/maps*",
        "https://*.google.com.au/maps*"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}